


CREATE TABLE EMP (
   EMPNO varchar(5) NOT NULL COMMENT '직원번호',
   ENAME varchar(30) NOT NULL COMMENT '직원명',
   JOB VARCHAR(2) NULL COMMENT '직급',
   MGRNO varchar(5) NULL COMMENT '관리번호',
   HIREDATE DATE NULL COMMENT '입사일자',
   SAL INT UNSIGNED NULL COMMENT '급여',
   COMM NUMERIC NULL COMMENT '공통',
   DEPTNO VARCHAR(4) NULL COMMENT '부서번호',
   CONSTRAINT EMP_PK PRIMARY KEY (EMPNO,ENAME)
)
DEFAULT CHARSET=utf8mb4;


CREATE TABLE COMM_CODE (
   CODE varchar(10) NOT NULL,
   CODE_NAME varchar(64) NULL,
   SUB_CODE varchar(10) NOT NULL,
   SUB_CODE_NAME varchar(64) NULL,
   ETC1 varchar(256) NULL,
   ETC2 varchar(256) NULL,
   ETC3 varchar(256) NULL,
   ETC4 varchar(256) NULL,
   ETC5 varchar(256) NULL,
   CONSTRAINT COMM_CODE_PK PRIMARY KEY (CODE,SUB_CODE)
)
DEFAULT CHARSET=utf8mb4;


